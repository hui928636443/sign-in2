{
  "manifest_version": 3,
  "name": "NewAPI 签到配置提取器",
  "version": "2.2.0",
  "description": "一键提取 NewAPI 站点和 LinuxDO 的 Cookie，生成签到配置",
  "permissions": [
    "cookies",
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "*://*.linux.do/*",
    "*://linux.do/*",
    "*://*.wzw.pp.ua/*",
    "*://*.h-e.top/*",
    "*://*.kfc-api.sxxe.net/*",
    "*://*.free.duckcoding.com/*",
    "*://*.runanytime.hxi.me/*",
    "*://*.ai.zzhdsgsss.xyz/*",
    "*://*.api.mitchll.com/*",
    "*://*.anyrouter.top/*",
    "*://*.new-api-bxhm.onrender.com/*",
    "*://*.aidrouter.qzz.io/*",
    "*://*.lightllm.online/*",
    "*://*.api.hotaruapi.top/*",
    "*://*.api.dev88.tech/*",
    "*://*.ai.huan666.de/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
